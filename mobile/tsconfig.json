{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "*": [
        "src/types/*"
      ]
    },
    "typeRoots": [
      "./src/types",
      "./node_modules/@types"
    ],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "bundler",
    "ignoreDeprecations": "5.0"
  }
}